{
  "name": "thesis-master-example",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://github.com/kralmajster/thesis-aws-example",
  "description": "Master thesis AWS implementation example using serverless framework",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev npx sls offline --dontPrintOutput",
    "logs": "npx sls logs -f",
    "invoke": "npx sls invoke local -f",
    "deploy:fn": "npx tsc && npx sls deploy function -f",
    "deploy:stack": "npx sls deploy",
    "delete:stack": "npx sls remove",
    "run:script": "webpack --config script.webpack.config.js --mode development && node --max-old-space-size=4096 __generated/build/_scripts/script-output.js"
  },
  "dependencies": {
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.307.0",
    "dotenv": "^6.0.0",
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0-rc.1",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@cellense/dotenv-plugin-serverless": "^1.0.0",
    "@types/aws-lambda": "^8.10.5",
    "babel-loader": "^8.0.2",
    "babel-plugin-node-source-map-support": "^0.0.1",
    "command-line-args": "^5.0.2",
    "cross-env": "^5.1.6",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "imports-loader": "^0.8.0",
    "prettier": "^1.12.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "serverless": "^1.27.3",
    "serverless-iam-roles-per-function": "^1.0.4",
    "serverless-offline": "^3.25.10",
    "serverless-pseudo-parameters": "^2.1.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.1.5",
    "ts-loader": "^5.0.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
